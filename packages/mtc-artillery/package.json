  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "next dev --port=8888",
    "start": "next start",
    "build:analyze": "cross-env ANALYZE=true npm run build:unsafe",
    "build:prod": "npm run lint && npm run build:unsafe",
    "build:unsafe": "npm run convert && npm run next:build",
    "next:build": "next build",
    "lint": "npm run next:lint && npm run eslint",
    "next:lint": "next lint",
    "eslint": "eslint .",
    "lint:fix": "npm run eslint -- --fix",
    "format": "prettier . -w",
    "fix": "npm run eslint -- --fix && npm run format",
    "depcheck": "depcheck . --quiet --skip-missing",
    "convert": "node scripts/convert/index.mjs"
  },
  "dependencies": {
    "2dec": "^1.0.7",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.13",
    "@mui/joy": "5.0.0-beta.51",
    "@mui/material": "^5.16.13",
    "@tauri-apps/api": "^2.2.0",
    "flag-icons": "^7.2.3",
    "immer": "^10.1.1",
    "is-image": "^4.0.0",
    "klaw": "^4.1.0",
    "merge-sx": "^3.3.1",
    "next": "15.1.2",
    "next-intl": "^3.26.3",
    "react": "^18.3.1",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-zoom-pan-pinch": "^3.6.1",
